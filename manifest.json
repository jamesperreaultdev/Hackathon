{
    "manifest_version": 2,
    "name": "CensorFlow",
    "description": "An advanced, easily customizable chat filter.",
    "version": "0.5",
    "icons":{
        "128": "icon128.png",
        "32": "icon32.png",
        "48": "icon48.png"
        },
   
    "permissions": ["tabs","<all_urls>","activeTab","audioCapture"],
     
    "browser_action":{
        "default_icon": {   
                         
          "16": "icon16.png",           
          "24": "icon24.png",           
          "32": "icon32.png"            
        },
        "default_title": "yeye",     
        "default_popup": "popup.html"     
      },
      "content_scripts": [{
        "js": ["censor.js"],
        "matches":["<all_urls>"]
         
    }]
  }
  
